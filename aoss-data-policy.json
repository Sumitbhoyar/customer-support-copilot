[
  {
    "Rules": [
      {
        "Resource": ["collection/kb-dev"],
        "Permission": [
          "aoss:CreateCollectionItems",
          "aoss:UpdateCollectionItems",
          "aoss:DescribeCollectionItems"
        ],
        "ResourceType": "collection"
      },
      {
        "Resource": ["index/kb-dev/*"],
        "Permission": [
          "aoss:CreateIndex",
          "aoss:DescribeIndex",
          "aoss:ReadDocument",
          "aoss:WriteDocument",
          "aoss:UpdateIndex",
          "aoss:DeleteIndex"
        ],
        "ResourceType": "index"
      }
    ],
    "Principal": [
      "arn:aws:iam::602320639652:user/sumit-bhoyar",
      "arn:aws:iam::602320639652:role/bedrock-kb-dev"
    ],
    "Description": "Data access for KB and index creation"
  }
]
